{
  "title": "LeetCode for TypeScript",
  "author": "yalda",
  "description": "LeetCode problems' solutions with TypeScript",
  "styles": {
    "website": "assets/css/website.css"
  },
  "plugins": [
    "-name",
    "-sharing",
    "-lunr",
    "-search",
    "search-plus",
    "splitter",
    "github-buttons",
    "-highlight",
    "prism",
    "prism-themes",
    "3-ba"
  ],
  "pluginsConfig": {
    "fontsettings": {
      "theme": "night",
      "family": "serif",
      "size": 2
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "guocaoyi",
          "repo": "leetcode-ts",
          "type": "star",
          "size": "smal",
          "count": true
        }
      ]
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ]
    },
    "3-ba": {
      "token": "8b86dbbab489aa49fb5fb7885a8953cc"
    }
  }
}